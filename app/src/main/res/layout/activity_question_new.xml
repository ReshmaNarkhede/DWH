<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".activity.question.QuestionNewActivity">

    <com.stepstone.stepper.StepperLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/stepperLayout"
        app:ms_backButtonColor="@color/white"
        app:ms_nextButtonColor="@color/white"
        app:ms_completeButtonColor="@color/colorPrimaryDark"
        app:ms_stepperType="dots"
        app:ms_activeStepColor="@color/white"
        app:ms_inactiveStepColor="@color/white"
        app:ms_showErrorStateOnBack="true">
    </com.stepstone.stepper.StepperLayout>
</androidx.constraintlayout.widget.ConstraintLayout>